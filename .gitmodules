[submodule "$HOME/.config/zsh/plugins/zsh-defer"]
	path = $HOME/.config/zsh/plugins/zsh-defer
	url = https://github.com/romkatv/zsh-defer.git
	branch = master
[submodule "$HOME/.config/zsh/plugins/fast-syntax-highlighting"]
	path = $HOME/.config/zsh/plugins/fast-syntax-highlighting
	url = https://github.com/zdharma/fast-syntax-highlighting.git
	branch = main
[submodule "$HOME/.config/zsh/plugins/zsh-history-substring-search"]
	path = $HOME/.config/zsh/plugins/zsh-history-substring-search
	url = https://github.com/zsh-users/zsh-history-substring-search.git
	branch = master
[submodule "$HOME/.config/zsh/plugins/zsh-autosuggestions"]
	path = $HOME/.config/zsh/plugins/zsh-autosuggestions
	url = https://github.com/zsh-users/zsh-autosuggestions.git
	branch = master
[submodule "$HOME/.config/zsh/plugins/zsh-completions"]
	path = $HOME/.config/zsh/plugins/zsh-completions
	url = https://github.com/zsh-users/zsh-completions.git
	branch = master
